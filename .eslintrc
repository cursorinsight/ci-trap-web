{

  "globals-were-intentionally-removed": ["by", "bence"],

  "no-empty": 1,
  "no-unreachable": 2,
  "use-isnan": 1,
  "valid-jsdoc": 2,

  "curly": 2,
  "default-case": 1,
  "eqeqeq": [2, "smart"],
  "no-else-return": 1,
  "no-unused-expressions": 1,

  "no-shadow": 1,
  "no-undefined": 2,
  "no-undef-init": 2,
  "no-unused": 2,
  "no-use-before-define": 2,

  "no-mixed-requires": 2,
  "no-new-require": 2,

  "brace-style": [2, "stroutrup"],
  "eof-last": 2,
  "no-lonely-if": 2,
  "no-spaced-func": 0,
  "no-space-before-semi": 2,
  "no_trailing-spaces": 2,
  "no-underscore-dangle": 0,
  "no-mixed-spaces-and-tabs": 2,
  "quotes": [2, "double", "avoid-escape"],
  "space-after-keywords": 2,
  "space-in-brackets": [2, "never"],
  "space-return-throw-case": 2,
  "space-unary-word-ops": 2,
  "one-var": 2,

  "camelcase": 2,
  "new-cap": 2,
}
